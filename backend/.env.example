# MongoDB Connection String
# Get this from MongoDB Atlas or your local MongoDB installation
MONGO_URI=mongodb+srv://username:password@cluster.mongodb.net/database_name?retryWrites=true&w=majority

# JWT Secret Key
# Generate a strong random string for production
JWT_SECRET=your_very_strong_and_secret_jwt_key_here

# Node Environment
NODE_ENV=development

# Server Configuration
PORT=3000
HOST=0.0.0.0

# Firebase Configuration
# Add your serviceAccountKey.json file to the backend directory
# This file should be downloaded from Firebase Console > Project Settings > Service Accounts

